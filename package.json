{
  "name": "nadinCheck",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "simple-git-hooks",
    "test": "NODE_OPTIONS=--no-deprecation vitest",
    "test:unit": "NODE_OPTIONS=--no-deprecation vitest test/unit",
    "test:e2e": "cypress run",
    "test:server": "NODE_OPTIONS=--no-deprecation vitest test/server"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "@mdi/font": "^7.4.47",
    "@nuxt/eslint": "1.12.1",
    "@nuxt/hints": "1.0.0-alpha.5",
    "@nuxt/test-utils": "3.21.0",
    "@nuxtjs/i18n": "10.2.1",
    "@tailwindcss/vite": "^4.1.18",
    "bcryptjs": "^3.0.3",
    "drizzle-orm": "^0.45.1",
    "eslint": "^9.0.0",
    "jsonwebtoken": "^9.0.3",
    "nuxt": "^4.2.2",
    "tailwindcss": "^4.1.18",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "cypress": "^15.8.1",
    "drizzle-kit": "^0.31.8",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "simple-git-hooks": "^2.13.1",
    "vite-plugin-vuetify": "^2.1.2",
    "vitest": "^4.0.16",
    "vuetify": "^3.11.6"
  },
  "lint-staged": {
    "*.{js,ts,vue,json,css,scss,md}": [
      "eslint . --fix",
      "prettier . --write"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "bunx lint-staged"
  }
}
